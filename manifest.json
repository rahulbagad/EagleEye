{
    "name": "Eagle Eye",
    "version": "1.0",
    "description": "Dark mode for websites",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
        "scripts": ["script.js"],
        "persistent": false
    },
    "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "img/dark.png",
        "32": "img/dark.png",
        "48": "img/dark.png",
        "128": "img/dark.png"
    }
    },
    "manifest_version": 2
}